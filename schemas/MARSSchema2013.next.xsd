        <xs:annotation>
          <xs:documentation>all available OS services</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RunningServices" type="aae:Services"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>OS services which need to be running
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisabledServices" type="aae:Services"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>disabled OS services</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ManualServices" type="aae:Services"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>OS services which need to start manual
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="LogInformation">
    <xs:attribute name="Path" type="aae:Path" use="required">
      <xs:annotation>
        <xs:documentation>path of logfiles</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ErrorLogPattern" type="aae:Pattern">
      <xs:annotation>
        <xs:documentation>pattern to find error log files in $LOGPATH, e.g. "error-??-??-??"
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="AccessLogPattern" type="aae:Pattern">
      <xs:annotation>
        <xs:documentation>pattern to find access log files in $LOGPATH, e.g. "access-??-??-??"
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="DefaultLogPattern" type="aae:Pattern">
      <xs:annotation>
        <xs:documentation>pattern to find default log file, e.g. *.log</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="SoftwareInformation">
    <xs:all>
      <xs:element name="LogInformation" type="aae:LogInformation"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about logging of software
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceContractInformation" type="aae:ServiceContractInformation"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about service contracts
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Vendor" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>vendor of software</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Name" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>name of software product</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MajorVersion" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>major verison of software (e.g. 10.2)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MinorVersion" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>minor version of software (e.g. 0.5.0)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="PatchLevel" type="aae:ShortString" />
    <xs:attribute name="License" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>license information for software
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="InstallPath" type="aae:Path">
      <xs:annotation>
        <xs:documentation>Software is installed in path. In case of
          software instances, specify location of instance
          configuration
          files, e.g.
          /opt/apache22/server/webserver1.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="SLATimes">
    <xs:annotation>
      <xs:documentation>
        SLA operation
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="ID" type="aae:ID" />
    <xs:attribute name="Name" type="aae:ShortString" />
  </xs:complexType>

  <!-- SLA -->
  <xs:complexType name="SLA">
    <xs:annotation>
      <xs:documentation>
        service level agreement information
      </xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="OperationTimes" type="aae:SLATimes"
                  minOccurs="0" />
      <xs:element name="FrozenZone" type="aae:SLATimes"
                  minOccurs="0" />
      <xs:element name="MaintenanceSlot" type="aae:SLATimes"
                  minOccurs="0" />
    </xs:all>
    <xs:attribute name="ID" type="aae:ID" use="required" />
    <xs:attribute name="Type" type="aae:SLAType" use="required" />
  </xs:complexType>

  <xs:simpleType name="RackUnit">
    <xs:annotation>
      <xs:documentation>
        the position of the system in a rack
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0"></xs:minInclusive>
      <xs:maxInclusive value="45"></xs:maxInclusive>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RAM">
    <xs:annotation>
      <xs:documentation>ammount of memory</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Speed">
    <xs:annotation>
      <xs:documentation>speed of a diskdrive</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="5400" />
      <xs:enumeration value="7200" />
      <xs:enumeration value="10000" />
      <xs:enumeration value="15000" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="CPUSpeed">
    <xs:annotation>
      <xs:documentation>speed of CPU</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:positiveInteger" />
  </xs:simpleType>

  <xs:simpleType name="Size">
    <xs:annotation>
      <xs:documentation>size of a diskdrive or filesystem in Mbytes</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SpareDisk">
    <xs:annotation>
      <xs:documentation>
        is this disk a spare drive?
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Yes" />
      <xs:enumeration value="No" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RaidLevel">
    <xs:annotation>
      <xs:documentation>raid level of a raid</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="5" />
      <xs:enumeration value="6" />
      <xs:enumeration value="10" />
      <xs:enumeration value="50" />
      <xs:enumeration value="60" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="DiskMembers">
    <xs:annotation>
      <xs:documentation>
        which disks are member of a LUN (logical unit)
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SLAType">
    <xs:annotation>
      <xs:documentation>
        type of service level agreement
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Service Based" />
      <xs:enumeration value="Customer Based" />
      <xs:enumeration value="Multi Level" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="StartDate">
    <xs:annotation>
      <xs:documentation>a start date</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="EndDate">
    <xs:annotation>
      <xs:documentation>a end date</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="GW">
    <xs:annotation>
      <xs:documentation>default network gateway</xs:documentation>
    </xs:annotation>
    <xs:restriction base="aae:IP">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Destination">
    <xs:annotation>
      <xs:documentation>
        destination of a network route
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="aae:RouteDestination">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Gateway">
    <xs:annotation>
      <xs:documentation>
        gateway for a specific network route
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="aae:IP">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Path">
    <xs:annotation>
      <xs:documentation>a path</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" />
  </xs:simpleType>

  <xs:simpleType name="Protocol">
    <xs:annotation>
      <xs:documentation>network protocol</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="TCP" />
      <xs:enumeration value="UDP" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Port">
    <xs:annotation>
      <xs:documentation>
        a network port
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:int">
      <xs:minExclusive value="1" />
      <xs:maxExclusive value="65536" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="HasAgentType_WatchMe">
    <xs:annotation>
      <xs:documentation>monitoring agent type</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="True" />
      <xs:enumeration value="False" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Pattern">
    <xs:annotation>
      <xs:documentation>regex pattern to identify a log file
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string" />
  </xs:simpleType>

  <!-- Element and Attribute Definition common for all Node Levels -->

  <xs:complexType name="DependenciesType">
    <xs:complexContent>
      <xs:annotation>
        <xs:documentation>dependencies to next MARS layer
        </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:anyType">
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
          <xs:element name="Node">
            <xs:complexType>
              <xs:attribute name="ID" type="aae:aaeID" use="required" />
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="CustomerInformationType">
    <xs:complexContent>
      <xs:annotation>
        <xs:documentation>information about the customer</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:anyType">
        <xs:attribute name="ID" type="aae:customerId" use="required" />
        <xs:attribute name="Name" type="aae:ShortString" />
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ExtensionsType">
    <xs:complexContent>
      <xs:annotation>
        <xs:documentation>
          extensions not included in standard MARS schema
        </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:anyType">
        <xs:sequence>
          <xs:any minOccurs="0" maxOccurs="unbounded"
                  processContents="lax" />
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="CommonNodeType">
    <xs:sequence>
      <xs:element name="Dependencies" type="aae:DependenciesType" minOccurs="0" />
      <xs:element name="CustomerInformation" type="aae:CustomerInformationType" />
      <xs:element name="Extensions" type="aae:ExtensionsType" minOccurs="0" />      
    </xs:sequence>
    <xs:attribute name="ID" type="aae:aaeID" use="required">
      <xs:annotation>
        <xs:documentation>
          company:project:(Machine|Application|Resource|Software):name
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="NodeName" type="aae:NameString" use="required">
      <xs:annotation>
        <xs:documentation>human readable name of node</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="SLA" type="aae:SLAType">
      <xs:annotation>
        <xs:documentation>information about service level agreements</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Clustertype" type="aae:ClustertypeType" />
    <xs:attribute name="NominalState" type="aae:StatusType" />
    <xs:attribute name="MonitoringStatus" type="aae:StatusType" />
    <xs:attribute name="MonitoringStatusMessage" type="xs:string" />
    <xs:attribute name="EventCounter.Warning" type="xs:int" />
    <xs:attribute name="EventCounter.Critical" type="xs:int" />
  </xs:complexType>

  <xs:complexType name="MachineAttributes">
    <xs:complexContent>
      <xs:extension base="aae:CommonNodeType">
        <xs:sequence>
          <xs:element name="ContactInformation" minOccurs="0">
            <xs:annotation>
              <xs:documentation>contact information data</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="HardwareC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of hardware responsible
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="TechC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of technical responsible
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ServiceC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of service manager
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="LocationInformation" type="aae:LocationInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about the node location
                (datacenter, rack)
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="HardwareInformation" type="aae:HardwareInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about the machine hardware
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OSInformation" type="aae:OSInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about operating system
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OSServiceInformation" type="aae:OSServiceInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about OS services
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NetworkInformation" type="aae:NetworkInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about the machines network
                connections and routes
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StorageInformation" type="aae:StorageInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about filesystems, disks, partitions and all that
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="NodeType" type="aae:NodeType" fixed="Machine" use="required">
          <xs:annotation>
            <xs:documentation>
              DataType of node, attribute has a fixed value
              of Machine
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="HasAgentType_WatchMe" type="aae:HasAgentType_WatchMe">
          <xs:annotation>
            <xs:documentation>is a arago WatchMe agent installed on this
              system?
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MachineArchitecture" type="aae:MachineArchitecture"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="SoftwareAttributes">
    <xs:complexContent>
      <xs:extension base="aae:CommonNodeType">
        <xs:sequence>
          <xs:element name="ContactInformation" minOccurs="0">
            <xs:annotation>
              <xs:documentation>contact information data</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="SoftwareC" type="aae:Contact"
                            minOccurs="0" />
                <xs:element name="TechC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of technical responsible
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ServiceC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of service manager
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="SoftwareInformation" type="aae:SoftwareInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about software
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NetworkInformation" type="aae:SoftwareNetworkInformation"
                      minOccurs="0">
            <xs:annotation>
              <xs:documentation>information about the machines network
                connections and routes
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SoftwareConfiguration" minOccurs="0">
            <xs:complexType>
              <xs:attribute name="Instance" type="xs:string">
                <xs:annotation>
                  <xs:documentation>instancename general. you need to model each instance as a separate software node</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="BasePath" type="xs:string">
                <xs:annotation>
                  <xs:documentation>base installation path
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="User" type="xs:string">
                <xs:annotation>
                  <xs:documentation>user credentials - username
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="Password" type="xs:string">
                <xs:annotation>
                  <xs:documentation>user credentials - password
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="DB" type="xs:string">
                <xs:annotation>
                  <xs:documentation>name of operated database</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="StartScript" type="xs:string">
                <xs:annotation>
                  <xs:documentation>path of the software startscript
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="ConfigFile" type="xs:string">
                <xs:annotation>
                  <xs:documentation>path of the software config file
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="ProcessName" type="xs:string">
                <xs:annotation>
                  <xs:documentation>name of the started software in the process
                    table
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="ProcessUser" type="xs:string">
                <xs:annotation>
                  <xs:documentation>name of service/software user account
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="NetworkPort" type="aae:Port">
                <xs:annotation>
                  <xs:documentation>listening port of the software
                  </xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="NodeType" type="aae:NodeType" fixed="Software" use="required">
          <xs:annotation>
            <xs:documentation>
              DataType of node, attribute has a fixed value
              of Software
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResourceAttributes">
    <xs:complexContent>
      <xs:extension base="aae:CommonNodeType">
        <xs:sequence>
          <xs:element name="ContactInformation" minOccurs="0">
            <xs:annotation>
              <xs:documentation>contact information data</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="ResourceC" type="aae:Contact"
                            minOccurs="0" />
                <xs:element name="TechC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of technical responsible
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ServiceC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of service manager
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="NodeType" type="aae:NodeType" fixed="Resource" use="required">
          <xs:annotation>
            <xs:documentation>
              DataType of node, attribute has a fixed value
              of Resource
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ApplicationAttributes">
    <xs:complexContent>
      <xs:extension base="aae:CommonNodeType">
        <xs:sequence>
          <xs:element name="ContactInformation" minOccurs="0">
            <xs:annotation>
              <xs:documentation>contact information data</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="ApplicationC" type="aae:Contact"
                            minOccurs="0" />
                <xs:element name="TechC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of technical responsible
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ServiceC" type="aae:Contact"
                            minOccurs="0">
                  <xs:annotation>
                    <xs:documentation>contact information of service manager
                    </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="NodeType" type="aae:NodeType" fixed="Application" use="required">
          <xs:annotation>
            <xs:documentation>
              DataType of node, attribute has a fixed value
