          <xs:documentation>machine location in datacenter</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="OSInformation">
    <xs:attribute name="Vendor" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>vendor of operating system</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Name" type="aae:ShortString" use="required" />
    <xs:attribute name="MajorVersion" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>major version operating system</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MinorVersion" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>minor version operating system</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="PatchLevel" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>patch level of operating system (e.g. service
          pack 3)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Lang" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>operating system language</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="License" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>optional license information for operating system
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="BalancingType">
    <xs:sequence>
      <xs:element name="Real-IP" minOccurs="0"
                  maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="Address" type="aae:IP">
            <xs:annotation>
              <xs:documentation>real IP-address</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Virtual-IP" type="aae:IP">
      <xs:annotation>
        <xs:documentation>virtual IP-address of load balancer
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Session" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>session information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="RR" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>round robin information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="URL" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>URL information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Other" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>other information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="RuleType">
    <xs:attribute name="RuleName" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>unique rule name from config for quick find
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Source" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>source information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Destination" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>destination information</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Port" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>port or protocol</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="IPv4InterfaceType">
    <xs:attribute name="Name" type="aae:ShortString"  use="required">
      <xs:annotation>
        <xs:documentation>name of interface (e.g. eth0)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MAC" type="aae:MAC_address">
      <xs:annotation>
        <xs:documentation>MAC adress of network interface card (e.g.
          00:00:00:00:00:00)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IP" type="aae:IPv4_address">
      <xs:annotation>
        <xs:documentation>IPv4 address of interface (e.g. 10.251.30.40)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Netmask" type="aae:IPv4_address">
      <xs:annotation>
        <xs:documentation>netmask address of interface (e.g. 255.255.255.0)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="IPv6InterfaceType">
    <xs:attribute name="Name" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>name of interface (e.g. eth0)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="MAC" type="aae:MAC_address">
      <xs:annotation>
        <xs:documentation>MAC adress of network interface card (e.g.
          00:00:00:00:00:00)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IPv6" type="aae:IPv6_address">
      <xs:annotation>
        <xs:documentation>IPv6 address of interface
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="PrefixLength" type="aae:IPv6_prefixLength">
      <xs:annotation>
        <xs:documentation>The prefix length of an IPv6 address (determines host/networ part)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="InterfaceInformation">
    <xs:annotation>
      <xs:documentation>information about network interfaces
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="Interface" type="aae:IPv4InterfaceType" />
      <xs:element name="InterfaceIPv6" type="aae:IPv6InterfaceType" />
      <xs:element name="VirtualInterface" type="aae:IPv4InterfaceType" />
      <xs:element name="VirtualInterfaceIPv6" type="aae:IPv6InterfaceType" />
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="BalancingInformation">
    <xs:annotation>
      <xs:documentation>information about load balancing config
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="Config" type="aae:BalancingType" />
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="RoutingInformation">
    <xs:annotation>
      <xs:documentation>information about configured network routes
        including default gateway (GW)
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Routes" type="aae:Routes" minOccurs="0"
                  maxOccurs="unbounded">
