      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="RAM" type="aae:RAM">
      <xs:annotation>
        <xs:documentation>ammount of RAM in MB</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="LocationInformation">
    <xs:all>
      <xs:element name="DataCenter" type="aae:Address">
        <xs:annotation>
          <xs:documentation>location of datacenter</xs:documentation>
        </xs:annotation>
      </xs:element>
