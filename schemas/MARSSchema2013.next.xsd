      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Model" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>model of hardware</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="SerialNo" type="aae:ShortString">
