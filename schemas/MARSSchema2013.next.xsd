    <xs:attribute name="GW" type="aae:GW" use="required">
      <xs:annotation>
        <xs:documentation>default gateway</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="SoftwareNetworkInformation">
    <xs:attribute name="IP" type="aae:ServiceIP" use="required">
      <xs:annotation>
        <xs:documentation>an IP4 address</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Protocol" type="aae:Protocol" use="required">
      <xs:annotation>
        <xs:documentation>network protocol type (tcp|udp)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Port" type="aae:Port" use="required">
      <xs:annotation>
        <xs:documentation>the ip port</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="NetworkInformation">
    <xs:all>
      <xs:element name="InterfaceInformation" type="aae:InterfaceInformation"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about network interfaces
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RoutingInformation" type="aae:RoutingInformation"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about configured network routes
            including default gateway (GW)
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

