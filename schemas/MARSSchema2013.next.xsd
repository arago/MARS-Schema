  <!-- Attribute Definitions -->
  <xs:complexType name="Contact">
    <xs:attribute name="Name" type="aae:ShortString" />
    <xs:attribute name="Function" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>function of employee</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Company" type="aae:LongString">
      <xs:annotation>
        <xs:documentation>company name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="eMail" type="aae:email">
      <xs:annotation>
        <xs:documentation>
          e-mail address of responsible
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Phone" type="aae:phone">
      <xs:annotation>
        <xs:documentation>
          phone number of responsible
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ADSUserName" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>
          a reference to user from ADS Directory. If this
          option is set,
          other attributes can be resolved
          dynamically and are
          not required.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="Address">
    <xs:attribute name="Name" type="aae:ShortString" use="required" >
      <xs:annotation>
        <xs:documentation>name of datacenter (e.g. DC1)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="City" type="aae:ShortString" ><!-- use="required" -->
      <xs:annotation>
        <xs:documentation>name of city</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Street" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>name of street and streetnumber
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Country" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>name of country (e.g. Germany)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="MachineLocation">
    <xs:attribute name="Room" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>room information (e.g. Room 2)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Rack" type="aae:ShortString" use="required">
      <xs:annotation>
        <xs:documentation>rack information (e.g. Rack J)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="RackUnit" type="aae:RackUnit" default="20">
      <xs:annotation>
        <xs:documentation>node location in rack (e.g. 20)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="CPUInfo">
    <xs:attribute name="Model" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>CPU model (e.g. Intel Xeon 5500)
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Speed" type="aae:CPUSpeed" >
      <xs:annotation>
        <xs:documentation>speed of CPU in MHz</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Quantity" type="xs:positiveInteger">
      <xs:annotation>
        <xs:documentation>quantity of installed CPUs</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="PhysDisks">
    <xs:attribute name="ID" type="aae:ID" use="required" />
    <xs:attribute name="Speed" type="aae:Speed" />
    <xs:attribute name="Size" type="aae:Size" use="required" /> 
    <xs:attribute name="SpareDisk" type="aae:SpareDisk" default="No" />
  </xs:complexType>

  <xs:complexType name="DiskPart">
    <xs:attribute name="ID" type="aae:ID" use="required" />
    <xs:attribute name="Name" type="aae:ShortString" />
    <xs:attribute name="Size" type="aae:Size" use="required" />
  </xs:complexType>

  <xs:complexType name="LUNs">
    <xs:attribute name="ID" type="aae:ID" use="required" />
    <xs:attribute name="Size" type="aae:Size"  use="required" />
    <xs:attribute name="RaidLevel" type="aae:RaidLevel" />
    <xs:attribute name="DiskMembers" type="aae:DiskMembers" />
  </xs:complexType>

  <xs:complexType name="Routes">
    <xs:attribute name="Destination" type="aae:Destination" use="required"/>
    <xs:attribute name="Gateway" type="aae:Gateway" use="required"/>
    <xs:attribute name="Netmask" type="aae:IPv4_address" use="optional">
      <xs:annotation>
        <xs:documentation>Netmask is mandatory for IPv4 destinations not using prefix notation</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Interface" type="aae:ShortString" />
  </xs:complexType>

  <xs:complexType name="ServiceContractInformation">
    <xs:attribute name="ID" type="aae:ID" default="1" use="optional">
      <xs:annotation>
        <xs:documentation>ID of service contract</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Name" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>name of service contract</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Type" type="aae:ShortString">
      <xs:annotation>
        <xs:documentation>type of service contract</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="StartDate" type="aae:StartDate">
      <xs:annotation>
        <xs:documentation>start date of service contract</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="EndDate" type="aae:EndDate">
      <xs:annotation>
        <xs:documentation>end date of service contract</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="HardwareInformation">
    <xs:all>
      <xs:element name="CPUInformation" type="aae:CPUInfo"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about the CPU</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceContractInformation" type="aae:ServiceContractInformation"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>information about service contract
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Vendor" type="aae:ShortString">
